{
  "promptDelete": false,
  "attachmentFolderPath": "docs/assets",
  "userIgnoreFilters": [
    "site/",
    "resources/",
    "templates/",
    ".+\\.toml",
    ".+\\.yaml",
    ".+\\.py",
    ".+\\.lock"
  ],
  "livePreview": false,
  "trashOption": "system",
  "alwaysUpdateLinks": true,
  "showUnsupportedFiles": true,
  "useMarkdownLinks": true,
  "newLinkFormat": "relative"
}